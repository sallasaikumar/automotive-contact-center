# AWS Strands Multi-Agent Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# AWS Bedrock Agent Configuration
BEDROCK_AGENT_ID=your_bedrock_agent_id
BEDROCK_AGENT_ALIAS_ID=TSTALIASID

# SageMaker Unified Studio Configuration
SAGEMAKER_DOMAIN_ID=your_sagemaker_domain_id
SAGEMAKER_USER_PROFILE=your_user_profile

# Application Configuration
NODE_ENV=production
PORT=8080

# Optional: Advanced Configuration
STRAND_TIMEOUT=30000
MAX_CONCURRENT_STRANDS=10
AGENT_CORE_RETRY_ATTEMPTS=3